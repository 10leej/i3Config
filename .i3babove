# This is the i3blocks configuration file for the top bar. This also assumes a
# screen resolution of 1920x1080, in order for the text to fit properly.
#----------------------------------------------------------------------------------

separator_block_width=37
markup=none
color=#ffffff

[githubfollows]
label= GHF:
command=/bin/bash $HOME/.libi3bview 44
separator=false
interval=1800

[githubcontribs]
label= GHC:
command=/bin/bash $HOME/.libi3bview 45
separator=false
interval=1800

[ytsubcount]
label= YTS:
command=/bin/bash $HOME/.libi3bview 43
separator=false
interval=1800

[yttotalviews]
label= YTV:
command=/bin/bash $HOME/.libi3bview 46
separator=false
interval=1800

[upgradecount]
label= APT:
command=/bin/bash $HOME/.libi3bview 42
separator=false
interval=3600

[psacount]
label= PS:
command=/bin/bash $HOME/.libi3bview 38
separator=false
interval=1

[mocpinfo]
label= MOCP:
command=/bin/bash $HOME/.libi3bview 37
separator=false
interval=1

[usbcount]
label= USBs:
command=/bin/bash $HOME/.libi3bview 40
separator=false
interval=1

[usercount]
label= Users:
command=/bin/bash $HOME/.libi3bview 35
separator=false
interval=1

[mountcount]
label= Mounts:
command=/bin/bash $HOME/.libi3bview 34
separator=false
interval=1

[cpu_temp]
label= CPU:
command=/bin/bash $HOME/.libi3bview 10
separator=false
interval=1

[kernel_version]
label= Kernel:
command=/bin/bash $HOME/.libi3bview 28
separator=false
interval=3600

