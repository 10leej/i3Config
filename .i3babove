separator_block_width=31
markup=none
color=#ffffff

[githubtodos]
label= GHTODO:
command=/bin/bash $HOME/.libi3bview 51
separator=false
interval=60

[githubfollows]
label= GHF:
command=/bin/bash $HOME/.libi3bview 44
separator=false
interval=1800

[githubcontribs]
label= GHC:
command=/bin/bash $HOME/.libi3bview 45
separator=false
interval=1800

[ytsubcount]
label= YTS:
command=/bin/bash $HOME/.libi3bview 43
separator=false
interval=1800

[yttotalviews]
label= YTV:
command=/bin/bash $HOME/.libi3bview 46
separator=false
interval=1800

[upgradecount]
label= APT:
command=/bin/bash $HOME/.libi3bview 42
separator=false
interval=3600

[psacount]
label= PROC:
command=/bin/bash $HOME/.libi3bview 38
separator=false
interval=1

[usbcount]
label= USB:
command=/bin/bash $HOME/.libi3bview 40
separator=false
interval=1

[usercount]
label= USERS:
command=/bin/bash $HOME/.libi3bview 35
separator=false
interval=1

[mountcount]
label= MOUNTS:
command=/bin/bash $HOME/.libi3bview 34
separator=false
interval=1

[cpu_temp]
label= CPU-TEMP:
command=/bin/bash $HOME/.libi3bview 10
separator=false
interval=1

[kernel_version]
label= KERNEL:
command=/bin/bash $HOME/.libi3bview 28
separator=false
interval=3600

