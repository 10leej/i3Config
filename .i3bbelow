# This is the i3blocks configuration file for the bottom bar. This also assumes a
# screen resolution of 1920x1080, in order for the text to fit properly.
#----------------------------------------------------------------------------------

separator_block_width=30
markup=none
color=#ffffff

[source]
label= INPUT:
command=/bin/bash $HOME/.libi3bview 24
separator=false
interval=1

[sink]
label= OUTPUT:
command=/bin/bash $HOME/.libi3bview 26
separator=false
interval=1

[cpufreq]
label= USAGE:
command=/bin/bash $HOME/.libi3bview 49
separator=false
interval=1

[usedram]
label= RAM:
command=/bin/bash $HOME/.libi3bview 21
separator=false
interval=1

[disk]
label= HOME:
separator=false
command=/bin/bash $HOME/.libi3bview 17
interval=1

[disk]
label= ROOT:
separator=false
command=/bin/bash $HOME/.libi3bview 15
interval=1

[download_total]
label= DOWN:
command=/bin/bash $HOME/.libi3bview 14
separator=false
interval=1

[upload_total]
label= UP:
command=/bin/bash $HOME/.libi3bview 13
separator=false
interval=1

[listening_estab_tcp_count]
label= TCP-EL:
command=/bin/bash $HOME/.libi3bview 48
separator=false
interval=1

[date]
label= D/T:
command=/bin/bash $HOME/.libi3bview 47
separator=false
interval=1

# vim: noexpandtab colorcolumn=84 tabstop=8 noswapfile nobackup
